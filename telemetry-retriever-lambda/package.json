  "keywords": [],
  "author": "Zacchaeus Bolaji",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.576.0",
    "@aws-sdk/client-lambda": "^3.577.0",
    "aws-lambda": "^1.0.7"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/aws-lambda": "^8.10.137",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  },
  "jest": {
    "preset": "ts-jest",
